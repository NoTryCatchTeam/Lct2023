{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Видео",
        "Презентация",
        "Текст"
      ]
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "Lite",
        "Intermediate",
        "Hard"
      ]
    },
    "isOnline": {
      "type": "boolean",
      "default": true
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "description": {
      "type": "text"
    },
    "stream": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::stream.stream"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "lessons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lesson.lesson"
    }
